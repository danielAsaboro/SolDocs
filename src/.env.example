# Solana RPC endpoint (required)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Anthropic API key for Claude AI (required)
ANTHROPIC_API_KEY=sk-ant-...

# API server port (optional, default 3000)
API_PORT=3000

# Agent discovery interval in milliseconds (optional, default 300000 = 5 min)
AGENT_DISCOVERY_INTERVAL_MS=300000

# Webhook URL for doc completion notifications (optional)
# When set, a POST request is sent to this URL each time documentation is generated
WEBHOOK_URL=https://example.com/webhook
